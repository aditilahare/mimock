{
	"compilerOptions": {
		"target": "ES6",
		"jsx": "react-jsx",
		"module": "CommonJS",
		"baseUrl": "./",
		"paths": {
			"src/*": ["src/*"],
			"api/*": ["src/api/*"],
			"services/*": ["src/api/services/*"],
			"assets/*": ["src/assets/*"],
			"components/*": ["src/components/*"],
			"config/*": ["src/config/*"],
			"common/*": ["src/common/*"],
			"styles": ["/src/styles/components/*"],
			"assets": ["/src/assets/*"],
			"hooks": ["/src/hooks/*"],
			"atoms": ["/src/atoms/*"],
			"constants": ["/src/constants/*"]
		}
	}
}
